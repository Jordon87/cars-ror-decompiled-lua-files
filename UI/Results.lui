    szPlayer = GetLocalizedString("InGameUI", "STR_PLAYER")
    if showWinner == 1 then
      szYouWin = GetLocalizedString("InGameUI", "STR_WON_MSG")
      if iWinnerRacer == 0 then
      else
      end
    elseif GetNumPlayers() == 2 then
    else
    end
    iRacer = 0
    curTime = math.floor(getvar(iRacer .. "TotalRaceTime"))
    staticPenalty = getvar("SecondsPenaltyPerStaticObject") * getvar(iRacer .. "NumStaticObjectsHit")
    dynamicPenalty = getvar("SecondsPenaltyPerDynamicObject") * getvar(iRacer .. "NumDynamicObjectsHit")
    totalPenalty = staticPenalty + dynamicPenalty
    totalTime1 = curTime + totalPenalty
    player1Results = {
      " " .. FormatTimeStandardized(curTime),
      "+ " .. FormatTimeStandardized(dynamicPenalty),
      "+ " .. FormatTimeStandardized(staticPenalty),
      "= " .. FormatTimeStandardized(totalTime1)
    }
    if GetNumPlayers() == 2 then
      iRacer = 1
      curTime = math.floor(getvar(iRacer .. "TotalRaceTime"))
      staticPenalty = getvar("SecondsPenaltyPerStaticObject") * getvar(iRacer .. "NumStaticObjectsHit")
      dynamicPenalty = getvar("SecondsPenaltyPerDynamicObject") * getvar(iRacer .. "NumDynamicObjectsHit")
      totalPenalty = staticPenalty + dynamicPenalty
      totalTime2 = curTime + totalPenalty
      player2Results = {
        " " .. FormatTimeStandardized(curTime),
