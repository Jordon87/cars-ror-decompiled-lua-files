        "",
        szPlayer .. 1,
        szPlayer .. 2
      }, {
        GetLocalizedString("InGameUI", "STR_FINISH_TIME"),
        GetLocalizedString("InGameUI", "STR_CONE_PENALTY"),
        GetLocalizedString("InGameUI", "STR_WALL_PENALTY"),
        GetLocalizedString("InGameUI", "STR_TOTAL_TIME")
      }, player1Results, player2Results)
    else
      _ARG_0_:Initialize({
        "",
        szPlayer .. 1,
        szPlayer .. 2
      }, {
        GetLocalizedString("InGameUI", "STR_FINISH_TIME"),
        GetLocalizedString("InGameUI", "STR_CONE_PENALTY"),
